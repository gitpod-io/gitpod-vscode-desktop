{
	"name": "gitpod-desktop",
	"displayName": "Gitpod",
	"description": "Gitpod Support",
	"publisher": "gitpod",
	"version": "0.0.156",
	"license": "MIT",
	"icon": "resources/gitpod.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gitpod-io/gitpod-vscode-desktop"
	},
	"bugs": {
		"url": "https://github.com/gitpod-io/gitpod/issues"
	},
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"remote development",
		"remote"
	],
	"extensionKind": [
		"ui"
	],
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"activationEvents": [
		"onResolveRemoteAuthority:ssh-remote",
		"onCommand:gitpod.exportLogs",
		"onCommand:gitpod.api.autoTunnel",
		"onCommand:gitpod.installLocalExtensions",
		"onAuthenticationRequest:gitpod",
		"onUri",
		"onStartupFinished"
	],
	"contributes": {
		"authentication": [
			{
				"label": "Gitpod",
				"id": "gitpod"
			}
		],
		"configuration": [
			{
				"title": "Gitpod",
				"properties": {
					"gitpod.host": {
						"type": "string",
						"description": "Gitpod Service URL. Update this if you are using a Gitpod self-hosted installation.",
						"default": "https://gitpod.io/",
						"scope": "application"
					},
					"gitpod.remote.useLocalApp": {
						"type": "boolean",
						"description": "Use the local companion app to connect to a remote workspace.\nWarning: Connecting to a remote workspace using local companion app will be removed in the near future.",
						"default": false,
						"scope": "application"
					},
					"gitpod.lsshPort": {
						"type": "number",
						"description": "The port to use for the local SSH server.",
						"scope": "application"
					},
					"gitpod.lsshIpcPort": {
						"type": "number",
						"description": "The port to use for the local SSH ipc server.",
						"scope": "application"
					}
				}
			}
		],
		"commands": [
			{
				"command": "gitpod.exportLogs",
				"category": "Gitpod",
				"title": "Export all logs"
			},
			{
				"command": "gitpod.installLocalExtensions",
				"title": "Install Local Extensions...",
				"category": "Gitpod",
				"enablement": "gitpod.inWorkspace == true"
			},
			{
				"command": "gitpod.signIn",
				"category": "Gitpod",
				"title": "Sign In"
			},
			{
				"command": "gitpod.workspaces.refresh",
				"category": "Gitpod",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "gitpod.workspaces.connectInNewWindow",
				"category": "Gitpod",
				"title": "Open Workspace in New Window"
			},
			{
				"command": "gitpod.workspaces.connectInNewWindow_context",
				"category": "Gitpod",
				"title": "Open in New Window"
			},
			{
				"command": "gitpod.workspaces.connectInCurrentWindow",
				"category": "Gitpod",
				"title": "Connect to Workspace"
			},
			{
				"command": "gitpod.workspaces.connectInCurrentWindow_context",
				"category": "Gitpod",
				"title": "Start and Connect"
			},
			{
				"command": "gitpod.workspaces.connectInCurrentWindow_context@1",
				"category": "Gitpod",
				"title": "Connect"
			},
			{
				"command": "gitpod.workspaces.connectInCurrentWindow_inline",
				"category": "Gitpod",
				"title": "Start and Connect",
				"icon": "$(arrow-right)"
			},
			{
				"command": "gitpod.workspaces.connectInCurrentWindow_inline@1",
				"category": "Gitpod",
				"title": "Connect",
				"icon": "$(arrow-right)"
			},
			{
				"command": "gitpod.workspaces.stopWorkspace",
				"category": "Gitpod",
				"title": "Stop Workspace",
				"icon": "$(primitive-square)"
			},
			{
				"command": "gitpod.workspaces.stopWorkspace_context",
				"category": "Gitpod",
				"title": "Stop",
				"icon": "$(primitive-square)"
			},
			{
				"command": "gitpod.workspaces.stopWorkspace_inline",
				"category": "Gitpod",
				"title": "Stop",
				"icon": "$(primitive-square)"
			},
			{
				"command": "gitpod.workspaces.stopCurrentWorkspace_inline",
				"category": "Gitpod",
				"title": "Disconnect and Stop",
				"icon": "$(primitive-square)"
			},
			{
				"command": "gitpod.workspaces.stopCurrentWorkspace",
				"category": "Gitpod",
				"title": "Stop Current Workspace",
				"icon": "$(primitive-square)"
			},
			{
				"command": "gitpod.workspaces.openInBrowser",
				"category": "Gitpod",
				"title": "Open in Browser"
			},
			{
				"command": "gitpod.workspaces.openContext",
				"category": "Gitpod",
				"title": "Open Context"
			},
			{
				"command": "gitpod.workspaces.deleteWorkspace",
				"category": "Gitpod",
				"title": "Delete Workspace"
			},
			{
				"command": "gitpod.workspaces.deleteWorkspace_context",
				"category": "Gitpod",
				"title": "Delete"
			},
			{
				"command": "gitpod.workspaces.disconnect",
				"category": "Gitpod",
				"title": "Disconnect",
				"icon": "$(debug-disconnect)"
			}
		],
		"menus": {
			"statusBar/remoteIndicator": [
				{
					"command": "gitpod.installLocalExtensions",
					"group": "remote_00_gitpod_navigation@01",
					"when": "gitpod.inWorkspace == true"
				}
			],
			"view/title": [
				{
					"command": "gitpod.workspaces.refresh",
					"when": "view == gitpod-workspaces",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_inline",
					"when": "viewItem =~ /^gitpod-workspaces.workspace$/",
					"group": "inline@1"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_inline@1",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.running$/",
					"group": "inline@1"
				},
				{
					"command": "gitpod.workspaces.stopWorkspace_inline",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.running$/",
					"group": "inline@2"
				},
				{
					"command": "gitpod.workspaces.disconnect",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.+connected$/",
					"group": "inline@3"
				},
				{
					"command": "gitpod.workspaces.stopCurrentWorkspace_inline",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.+connected$/",
					"group": "inline@4"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_context",
					"when": "viewItem =~ /^gitpod-workspaces.workspace$/",
					"group": "navigation@1"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_context@1",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.running$/",
					"group": "navigation@1"
				},
				{
					"command": "gitpod.workspaces.connectInNewWindow_context",
					"when": "viewItem =~ /^gitpod-workspaces.workspace/",
					"group": "navigation@2"
				},
				{
					"command": "gitpod.workspaces.openInBrowser",
					"when": "viewItem =~ /^gitpod-workspaces.workspace/",
					"group": "navigation@3"
				},
				{
					"command": "gitpod.workspaces.openContext",
					"when": "viewItem =~ /^gitpod-workspaces.workspace/",
					"group": "navigation@4"
				},
				{
					"command": "gitpod.workspaces.stopWorkspace_context",
					"when": "viewItem =~ /^gitpod-workspaces.workspace.running$/",
					"group": "navigation@5"
				},
				{
					"command": "gitpod.workspaces.deleteWorkspace_context",
					"when": "viewItem =~ /^gitpod-workspaces.workspace/",
					"group": "destruction@1"
				}
			],
			"commandPalette": [
				{
					"command": "gitpod.workspaces.refresh",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.refresh",
					"when": "gitpod.authenticated == true"
				},
				{
					"command": "gitpod.workspaces.connectInNewWindow",
					"when": "gitpod.authenticated == true"
				},
				{
					"command": "gitpod.workspaces.connectInNewWindow_context",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow",
					"when": "gitpod.authenticated == true"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_context",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_context@1",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_inline",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.connectInCurrentWindow_inline@1",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.stopWorkspace",
					"when": "gitpod.authenticated == true && gitpod.inWorkspace != true"
				},
				{
					"command": "gitpod.workspaces.stopWorkspace_context",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.stopWorkspace_inline",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.stopCurrentWorkspace_inline",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.stopCurrentWorkspace",
					"when": "gitpod.authenticated == true && gitpod.inWorkspace == true"
				},
				{
					"command": "gitpod.workspaces.openInBrowser",
					"when": "gitpod.authenticated == true && gitpod.inWorkspace == true"
				},
				{
					"command": "gitpod.workspaces.openContext",
					"when": "gitpod.authenticated == true && gitpod.inWorkspace == true"
				},
				{
					"command": "gitpod.workspaces.deleteWorkspace",
					"when": "gitpod.authenticated == true && gitpod.inWorkspace != true"
				},
				{
					"command": "gitpod.workspaces.deleteWorkspace_context",
					"when": "false"
				},
				{
					"command": "gitpod.workspaces.disconnect",
					"when": "false"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gitpod-view",
					"title": "Gitpod",
					"icon": "resources/explorer.svg"
				}
			]
		},
		"views": {
			"gitpod-view": [
				{
					"id": "gitpod-login",
					"name": "Login",
					"icon": "$(squirrel)",
					"when": "gitpod.authenticated != true"
				},
				{
					"id": "gitpod-workspaces",
					"name": "Workspaces",
					"icon": "$(squirrel)",
					"when": "gitpod.authenticated == true"
				},
				{
					"id": "gitpod-workspace",
					"name": "Workspace",
					"icon": "$(squirrel)",
					"when": "false"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "gitpod-login",
				"when": "gitpod.authenticated != true",
				"contents": "You have not yet signed in with Gitpod\n[Sign in](command:gitpod.signIn)"
			}
		]
	},
	"main": "./out/extension.js",
	"segmentKey": "YErmvd89wPsrCuGcVnF2XAl846W9WIGl",
	"configcatKey": "gitpod",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile-proxy": "webpack --mode development",
		"watch-proxy": "webpack watch --mode development",
		"compile-ext": "tsc -b tsconfig.compile.json",
		"watch-ext": "tsc -b -w tsconfig.compile.json",
		"package": "vsce package --yarn",
		"lint": "eslint . --ext .ts",
		"test": "mocha -u tdd",
		"proto-gen": "cd ./src/proto && buf lint && buf generate"
	},
	"devDependencies": {
		"@bufbuild/buf": "^1.15.0-1",
		"@types/google-protobuf": "^3.7.4",
		"@types/js-yaml": "^4.0.5",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/proper-lockfile": "^4.1.2",
		"@types/semver": "^7.3.10",
		"@types/ssh2": "^0.5.52",
		"@types/tmp": "^0.2.1",
		"@types/uuid": "8.0.0",
		"@types/vscode": "1.75.0",
		"@types/webpack": "^5.28.0",
		"@types/ws": "^8.5.4",
		"@types/yazl": "^2.4.2",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"@vscode/vsce": "^2.18.0",
		"copy-webpack-plugin": "^11.0.0",
		"eslint": "^8.13.0",
		"eslint-plugin-header": "3.1.1",
		"eslint-plugin-jsdoc": "^19.1.0",
		"grpc-tools": "^1.12.4",
		"minimist": "^1.2.6",
		"mocha": "^10.0.0",
		"ts-loader": "^9.2.7",
		"ts-proto": "^1.140.0",
		"typescript": "^4.6.3",
		"webpack": "^5.42.0",
		"webpack-cli": "^4.7.2"
	},
	"dependencies": {
		"@gitpod/gitpod-protocol": "main-gha",
		"@gitpod/local-app-api-grpcweb": "main-gha",
		"@gitpod/public-api": "main-gha",
		"@gitpod/supervisor-api-grpc": "main-gha",
		"@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
		"@microsoft/dev-tunnels-ssh": "^3.11.24",
		"@microsoft/dev-tunnels-ssh-keys": "^3.11.24",
		"@microsoft/dev-tunnels-ssh-tcp": "^3.11.24",
		"@segment/analytics-node": "^1.0.0-beta.24",
		"configcat-node": "^8.0.0",
		"js-yaml": "^4.1.0",
		"long": "^5.2.1",
		"mustard-connect-node": "^0.11.2",
		"nice-grpc": "^2.1.3",
		"nice-grpc-common": "^2.0.1",
		"node-fetch-commonjs": "^3.2.4",
		"pkce-challenge": "^3.0.0",
		"prom-client": "^14.1.1",
		"proper-lockfile": "^4.1.2",
		"protobufjs": "^7.2.2",
		"semver": "^7.3.7",
		"ssh-config": "^4.1.6",
		"ssh2": "^1.10.0",
		"tmp": "^0.2.1",
		"uuid": "8.1.0",
		"ws": "^8.13.0",
		"yazl": "^2.5.1"
	}
}
