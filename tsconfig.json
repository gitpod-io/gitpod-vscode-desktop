{
	"compilerOptions": {
		"target": "es2022",
		"lib": [
			"ES2022",
			"DOM"
		],
		"module": "commonjs",
		"strict": true,
		"outDir": "./out",
		"sourceMap": true,
		"exactOptionalPropertyTypes": false,
		"useUnknownInCatchVariables": false,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
